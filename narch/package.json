{
  "name": "narch",
  "version": "1.0.0",
  "description": "node framework with any architecture",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "narch": "tsc && narch run",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "tsx": "npx tsx src/index.ts",
    "nodemon": "nodemon src/index.ts",
    "dev-decorators": "ts-node --esm src/controllers/homeController/index.ts"
  },
  "bin": {
    "narch": "index.js"
  },
  "keywords": [
    "framework",
    "node",
    "node.js",
    "architecture",
    "backend"
  ],
  "author": "Moein Rezaee",
  "license": "ISC",
  "dependencies": {
    "busboy": "^1.6.0",
    "fs-manager": "file:../modules/fs-manager",
    "narch-cli": "file:../modules/narch-cli"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
